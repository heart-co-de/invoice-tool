{
  "name": "invoice-tool",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0 --report-unused-disable-directives",
    "lint:report": "pnpm run lint --output-file eslint_report.json --format json || exit 0",
    "lint:fix": "pnpm lint --fix",
    "lint:prettier": "prettier --check .",
    "format": "prettier --write .",
    "supabase:pull": "source .env && source .env.local && supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/services/supabase.types.ts",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.12",
    "@heroicons/vue": "^2.0.16",
    "@supabase/supabase-js": "^2.10.0",
    "@tailwindcss/forms": "^0.5.3",
    "@vueuse/core": "^9.13.0",
    "ical": "^0.8.0",
    "lodash": "^4.17.21",
    "uuid-v9": "^0.3.5",
    "vue": "^3.2.47",
    "vue-query": "^1.26.0",
    "vue-router": "^4.1.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/ical": "^0.8.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.5",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-packagejson": "^2.4.10",
    "supabase": "^1.41.4",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.5.4",
    "vite": "^4.1.4",
    "vue-tsc": "^1.2.0"
  },
  "packageManager": "pnpm@8.15.1+sha256.245fe901f8e7fa8782d7f17d32b6a83995e2ae03984cb5b62b8949bfdc27c7b5"
}
